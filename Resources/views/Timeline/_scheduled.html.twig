{% embed '@BkstgTimeline/Timeline/default.html.twig' %}
  {% trans_default_domain 'BkstgScheduleBundle' %}
  {% block timeline_text %}
    <big><i class="fa fa-calendar"></i></big>
    <strong>{{ timeline_component_render(timeline, 'subject') }}</strong>
    {{ 'timeline.scheduled'|trans }}
    "{{ timeline_component_render(timeline, 'directComplement') }}"
    {{ 'timeline.in'|trans }}
    <em>{{ timeline_component_render(timeline, 'indirectComplement') }}</em>
  {% endblock %}
{% endembed %}
